      <th>Nameorfio</th>
      <th>Persontype</th>
      <th>Phonecontractnumber</th>
      <th>Internetcontractnumber</th>
      <th>Phonenumber</th>
      <th>Faxnumber</th>
      <th>Email</th>
      <th>Representposition</th>
      <th>Representfio</th>
      <th>Contactperson</th>
      <th>Inn</th>
      <th>Kpp</th>
      <th>Ogrn</th>
      <th>Okpo</th>
      <th>Okonkh</th>
